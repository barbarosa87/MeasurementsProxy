<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from MainService WSDL service -->

  <service name="Main" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://measure.ote.com"/>
      <parameter name="wsdlServiceElement" value="MainService"/>
      <parameter name="schemaQualified" value="http://measure.ote.com,http://types.measure.ote.com,http://xml.apache.org/xml-soap"/>
      <parameter name="wsdlServicePort" value="Main"/>
      <parameter name="className" value="com.ote.measure.MainSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Main"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getAvailableFlows" qname="operNS:getAvailableFlows" xmlns:operNS="http://measure.ote.com" returnQName="retNS:getAvailableFlowsReturn" xmlns:retNS="http://measure.ote.com" returnType="rtns:GetAvailableFlowsResponse" xmlns:rtns="http://types.measure.ote.com" soapAction="" >
        <parameter qname="pns:CLI" xmlns:pns="http://measure.ote.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:Symptom" xmlns:pns="http://measure.ote.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:User" xmlns:pns="http://measure.ote.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="getAvailableFlows"/>

      <typeMapping
        xmlns:ns="http://types.measure.ote.com"
        qname="ns:GetAvailableFlowsResponse"
        type="java:com.ote.measure.types.GetAvailableFlowsResponse"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
